    <!-- This preference simply launches an intent when selected. Use this UI sparingly, per
         design guidelines. -->
    <Preference
        android:title="@string/title_open_source_delegate_adapter"
        android:key="@string/key_open_source_delegate_adapter"
        android:summary="@string/text_summary_open_source_delegate_adapter"
        >
        <intent android:action="android.intent.action.VIEW"
            android:data="https://github.com/boybeak/DelegateAdapter"/>
    </Preference>
    <Preference
        android:title="@string/title_open_source_selector"
        android:key="@string/key_open_source_selector"
        android:summary="@string/text_summary_open_source_selector"
        >
        <intent android:action="android.intent.action.VIEW"
            android:data="https://github.com/boybeak/Selector"/>
    </Preference>
</PreferenceScreen>
